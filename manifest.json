{
  "manifest_version": 2,

  "name": "Gotta Close 'Em All",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts":
   [
      {
        "matches": ["*://*/*"],
        "js": ["jquery-2.1.3.min.js", "script.js"],
        "run_at": "document_end"
      }
   ]
}
